---
apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority-data: ${cluster_ca_certificate}
    server: ${server}
  name: ${cluster_name}
contexts:
- context:
    cluster: ${cluster_name}
    user: ${name}
  name: ${cluster_name}
current-context: ${cluster_name}
preferences: {}
users:
- name: ${name}
  user:
    client-certificate-data: ${client_certificate}
    client-key-data: ${client_key}
